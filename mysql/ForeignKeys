ALTER TABLE `Schedules` ADD CONSTRAINT `FK_DriverID` FOREIGN KEY (`Driver_ID`) REFERENCES `Users`(`ID`)
ALTER TABLE `DriverTimes` ADD CONSTRAINT `FK_UniID` FOREIGN KEY (`UniveristyID`) REFERENCES `Users`(`UniversityID`)
ALTER TABLE `StudentTimes` ADD CONSTRAINT `FK_StuID` FOREIGN KEY (`UniversityID`) REFERENCES `USERS`(`UniversityID`)





